node{
stage('SOM Checkout'){
git 'https://github.com/prabhavagrawal/example-voting-app.git'
}
stage('Compile-Package'){
def mvnHome = tool name: 'maven', type: 'maven'
cmd '"${mvnHome}/bin/mvn package"'
}

stage('SonarQube Analysis') {
def mvnHome = tool name: 'maven',type: 'maven'
withSonarQubeEnv('sonar-6') {
cmd '"${mvnHome}/bin/mvn sonar:sonar"'
}
}
}
